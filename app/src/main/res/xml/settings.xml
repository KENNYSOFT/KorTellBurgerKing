<PreferenceScreen
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:key="settings" >
	<PreferenceCategory
		android:key="information"
		android:title="@string/preference_information" >
		<Preference
			android:key="about"
			android:title="@string/full_name"
			android:summary="@string/copyright" />
		<Preference
			android:key="update"
			android:title="@string/preference_update" />
		<Preference
			android:key="link"
			android:title="@string/preference_link" >
			<intent
				android:action="android.intent.action.VIEW"
				android:data="market://details?id=kr.KENNYSOFT.KorTellBurgerKing" />
		</Preference>
	</PreferenceCategory>
</PreferenceScreen>